                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=16.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT product.product_key, product.product_code, product.name, product.description, product.title, artist.artist_name, product_type.product_type, format.format_name, product_category.product_category, media.media, product.unit_price, 
                  product.unit_cost, product_status.product_status AS status, product.source_system_code, product.create_timestamp, product.update_timestamp
FROM     product INNER JOIN
                  artist ON product.artist_key = artist.artist_key INNER JOIN
                  product_type ON product.product_type_key = product_type.product_type_key INNER JOIN
                  format ON product.format_key = format.format_key INNER JOIN
                  product_category ON product_type.product_category_key = product_category.product_category_key INNER JOIN
                  media ON format.media_key = media.media_key INNER JOIN
                  product_status ON product.product_status_key = product_status.product_status_key
WHERE (product.create_timestamp &gt;= ? AND product.create_timestamp &lt;?) 
    OR( product.update_timestamp &gt;=? AND product.update_timestamp &lt;?)</property>
